<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cadastro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_cadastro"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_cadastro_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Cadastro">
        <activity
            android:name=".funcionalidades.ActivityAlterar"
            android:exported="false" />
        <activity
            android:name=".funcionalidades.ActivityListar"
            android:exported="false" />
        <activity
            android:name=".funcionalidades.ActivityCriar"
            android:exported="false" />
        <activity
            android:name=".funcionalidades.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>